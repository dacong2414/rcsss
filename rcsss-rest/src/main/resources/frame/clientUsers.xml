<?xml version="1.0" encoding="UTF-8"?>
<!-- 
sqlNo：查询序列号
sqlTitle：列表显示名称
sqlTemp：列表显示jsp模板
extendWidth：扩展功能栏宽度<extendWidth>110px</extendWidth>
sqlDetial：查询正文sql
extendList：扩展功能
extendType：0.详情查询SQL，用于双击查看详情 1.每行显示 （需要替换字段）2.表头显示
fieldInfoList：字段属性
fieldType：字段类型:String,Date,Long,Money
queryType：查询类型
0.表示不提供查询；
1.精确查询 (value=某值)
2.模糊查询(value like %某值%) 
3.数字范围查询(5<=value<=10) 
4.时间日期查询(value=2012-10-11)  
5.时间范围查询 (如：2012-10-31<=value<=2012-12-25) 
6.下拉列表查询
where
 	ui.u_id=##uId##
and
	ui.title=##title##
 -->
<queryInfo>
    <sqlNo>clientUsers</sqlNo>
    <sqlTitle>诊室端用户</sqlTitle>
    <sqlTemp></sqlTemp>
    <extendWidth></extendWidth>
    <sqlDetial><![CDATA[
SELECT
	l.u_id,
	l.login_name,
	sr.role_name,
	l.gmt_create,
	(
		SELECT
			u.title
		FROM
			user_info u
		WHERE
			l.u_id = u.u_id
	) AS title ,
	 IFNULL(
		(
			SELECT
            1
			FROM
			 monitor_client_mapping mcm
			WHERE
		mcm.monitor_uid=##uId##
    AND
   mcm.client_uid=l.u_id  
		),
		0
	) AS ck_flag
FROM
	sys_roles sr,
	logon_info l,
	sys_users_roles sur
WHERE
	l.disable_flag = 1
AND sr.use_flag = 1
AND sur.user_id = l.u_id
AND sur.role_id=sr.role_id
AND sr.role_name =##roleName##

	]]></sqlDetial>
    <fieldList>
        <fieldInfo>
            <fieldName>u_id</fieldName>
            <showName>用户ID</showName>
            <showIndex>1</showIndex>
            <fieldType>Long</fieldType>
            <queryType>0</queryType>
            <csId></csId>
        </fieldInfo>
        <fieldInfo>
            <fieldName>title</fieldName>
            <showName>用户名称</showName>
            <showIndex>5</showIndex>
            <fieldType>String</fieldType>
            <queryType>0</queryType>
            <csId></csId>
        </fieldInfo>
        <fieldInfo>
            <fieldName>login_name</fieldName>
            <showName>登录名</showName>
            <showIndex>10</showIndex>
            <fieldType>String</fieldType>
            <queryType>2</queryType>
            <csId></csId>
        </fieldInfo>
        <fieldInfo>
            <fieldName>role_name</fieldName>
            <showName>角色名称</showName>
            <showIndex>15</showIndex>
            <fieldType>String</fieldType>
            <queryType>2</queryType>
            <csId></csId>
        </fieldInfo>
         <fieldInfo>
            <fieldName>ck_flag</fieldName>
            <showName>是否选中</showName>
            <showIndex>20</showIndex>
            <fieldType>Long</fieldType>
            <queryType>6</queryType>
            <csId>3</csId>
        </fieldInfo>
        <fieldInfo>
            <fieldName>gmt_create</fieldName>
            <showName>创建时间</showName>
            <showIndex>25</showIndex>
            <fieldType>Date</fieldType>
            <queryType>5</queryType>
            <csId></csId>
        </fieldInfo>
    </fieldList>
</queryInfo>
